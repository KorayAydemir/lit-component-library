    "@lit-labs/gen-wrapper-react": "^0.3.3",
    "@types/css-minimizer-webpack-plugin": "^3.2.0",
    "@types/node": "^20.6.4",
    "@types/terser-webpack-plugin": "^5.2.0",
    "@types/webpack": "^5.28.2",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "babel-loader": "^9.1.3",
