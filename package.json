{
  "name": "lit-component-library",
  "main": "build/main.js",
  "type": "module",
  "exports": {
    ".": "./build/main.js",
    "./react": "./build/reactMain.js"
  },
  "files": "build/",
  "version": "4.1.5",
  "private": true,
  "scripts": {
    "build": "npm run-script lint && cross-env NODE_ENV=production webpack --config webpack.prod.cjs",
    "build:stats": "npm run-script lint && cross-env NODE_ENV=production:stats webpack --config webpack.prod.cjs --profile --json > stats.json",
    "dev": "cross-env NODE_ENV=development webpack serve --hot --config webpack.dev.cjs",
    "lint": "eslint --fix ./src"
  },
  "sideEffects": [
    "./src/global.ts",
    "./src/components/**/*.ts"
  ],
